{
  "env": {
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "64000"
  },
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "permissions": {
    "allow": [
      "Bash(git:*)",
      "Bash(cargo +nightly fmt:*)",
      "Bash(cargo check:*)",
      "Bash(cargo tree:*)",
      "Bash(cargo metadata:*)",
      "Bash(cargo locate-project:*)",
      "Bash(gh:*)",
      "Bash(ls:*)",
      "Bash(wc:*)",
      "Bash(tree:*)",
      "Bash(which:*)",
      "Bash(pwd)",
      "Bash(mkdir:*)",
      "Bash(ln:*)",
      "Bash(file:*)",
      "Bash(stat:*)",
      "Bash(readlink:*)",
      "Bash(realpath:*)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:docs.rs)",
      "WebFetch(domain:doc.rust-lang.org)",
      "WebFetch(domain:crates.io)",
      "WebSearch"
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bash -c '\"$(command -v bun)\" \"$(ls -td \"$HOME/.claude/plugins/cache/claude-hud/claude-hud/\"*/ 2>/dev/null | head -1)src/index.ts\"'"
  },
  "enabledPlugins": {
    "rust-analyzer-lsp@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "code-review@claude-plugins-official": true,
    "frontend-design@claude-plugins-official": true,
    "claude-hud@claude-hud": true
  },
  "alwaysThinkingEnabled": true
}
